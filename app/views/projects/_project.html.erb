<%= form_for [@project] do |p|%>
	<div class = "form">
		<h3>Add New Project</h3>
		<ul>
	    <% @project.errors.full_messages.each do |error| %>
	    <li><%= error %></li>
	    <% end %>
	  </ul>
	  <div class = "form-group">
		  <p>
		  	<%= p.label :Project_name, class: "form-label" %><br/>
		    <%= p.text_field :project_name, class: "form-control", :required => true%>
		  </p>
		</div>
		<div>
		  <p>
		  	<%= p.label :Start_date, class: "form-label" %><br/>
		    <%= p.text_field :start_date, class: "form-control", :required => true%>
		  </p>
		</div>
		<div>
		  <p>
		  	<%= p.label :Target_end_date, class: "form-label" %><br/>
		    <%= p.text_field :target_end_date, class: "form-control", :required => true%>
		  </p>
		</div>
		<div class = "form-group">
		  <p>
		  	<%= p.label :final_end_date, class: "form-label" %><br/>
		    <%= p.text_field :final_end_date, class: "form-control", :required => true%>
		  </p>
		</div>
		<div class = "form-group">
			<p>
				<%= p.submit %>
			</p>
		</div>
	</div>
<% end %>
